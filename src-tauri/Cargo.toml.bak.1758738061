[package]
name = "batoulab-app"
version = "0.0.3"
edition = "2021"

# Dépendances principales (runtime)
[dependencies]
tauri = { version = "2.8.5", features = ["macros"] }
tauri-plugin-fs = "2.4.2"
tauri-plugin-http = "2.5.2"
tauri-plugin-opener = "2.5.0"
serde = { version = "1", features = ["derive"] }
serde_json = "1"
tokio = { version = "1", features = ["rt-multi-thread", "macros"] }

# Dépendances pour le build script (build.rs)
[build-dependencies]
tauri-build = "2.4.1"
