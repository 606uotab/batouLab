{
  "identifier": "http-mistral",
  "description": "HTTP allowlist for model providers",
  "permissions": [
    "http:default"
  ],
  "windows": ["main"],
  "platforms": ["linux", "macOS", "windows"],
  "scope": {
    "http": {
      "allow": [
        "https://api.mistral.ai/**",
        "https://api.openai.com/**",
        "https://api.anthropic.com/**"
      ]
    }
  }
}
